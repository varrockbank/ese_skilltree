        {
            "rubriek": "rubriek-inhoud",
            "titel": "Course description",
            "weergave": "IN",
            "velden": [
                {
                    "veld": "item-inhoud-1",
                    "titel": "Content",
                    "waarde": "<html><body>- Week 1: Refresh Object-oriented programming Java<br/>- Week 2: Working with interfaces<br/>- Week 3: Working with inheritance<br/>- Week 4: Working with data structures from the Collections API<br/>- Week 5-6: Libraries, modern Java features<br/>- Week 7: Summary, other programming languages (no exam material)<br/><br/>This course use knowledge from FEB21011S Introduction to Programming, and is required for FEB22013X Simulation, MINFEW16 Computer Science, and FEB23007 Data Structures and Algorithms.</body></html>"
                },
                {
                    "veld": "item-inhoud-2",
                    "titel": "Learning goals",
                    "waarde": "<html><body>- Demonstrate proficiency in Java's type system, including polymorphism, inheritance, subtypes, and supertypes.<br/>- Be aware object-oriented programming concepts and their use in creating modular and extensible code.<br/>- Obtain expertise on reading and writing files, and know the appropriate techniques to work with the Exception mechanism.<br/>- Comprehend the concept of contracts in the context of object-oriented programming and the Object class<br/>- Utilize Java's Collections API and apply Lists, Sets, and Maps in your own projects.<br/>- Understand functional interfaces, lambda expressions, method references, and Streams for data processing.<br/>- Develop the ability to quickly learn and use third-party libraries to solve complex problems.</body></html>"
                },
                {
                    "veld": "item-inhoud-3",
                    "titel": "Changes as compared to previous year",
                    "waarde": "<html><body><p>No major changes are planned compared to last year.</p></body></html>"
                },
                {
                    "veld": "item-inhoud-5",
                    "titel": "Extra",
                    "waarde": "<html><body>None</body></html>"
                }
            ]
