        {
            "rubriek": "rubriek-inhoud",
            "titel": "Course description",
            "weergave": "IN",
            "velden": [
                {
                    "veld": "item-inhoud-1",
                    "titel": "Content",
                    "waarde": "<html><body>- The time-value of money and appropriate discount rate.<br/>- The role of arbitrage in finance<br/>- The value of bonds and stocks<br/>- Market efficiency and financial markets as information processor.<br/>- Risk and return models.<br/>- Portfolio theory.<br/>- Options and futures.<br/><br/>This course uses knowledge of FEB21017X Introduction to Analysis, FEB21020X Micro-economics, FEB22023X Vector Calculus and FEB22003X Introduction to Multivariate Statistics, and is required for FEB23006 Quantitative Models in Finance.</body></html>"
                },
                {
                    "veld": "item-inhoud-2",
                    "titel": "Learning goals",
                    "waarde": "<html><body>- Use the basic principles of finance to take investment decisions.<br/>- Calculate the fundamental value of a number of financial instruments.<br/>- Argue about the implications of efficient markets.<br/>- Compute expected returns using risk-factor models.</body></html>"
                }
            ]
